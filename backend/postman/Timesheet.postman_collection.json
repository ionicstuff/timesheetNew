  "item": [
    {
      "name": "Auth",
      "item": [
        {
          "name": "Register",
          "request": {
            "auth": { "type": "noauth" },
            "method": "POST",
            "header": [
              { "key": "Content-Type", "value": "application/json" }
            ],
            "url": "{{base_url}}/api/auth/register",
            "body": {
              "mode": "raw",
              "raw": "{\n  \"firstName\": \"John\",\n  \"lastName\": \"Doe\",\n  \"email\": \"john.doe@example.com\",\n  \"password\": \"Passw0rd!\"\n}"
            }
          }
        },
        {
          "name": "Login (sets token)",
          "event": [
            {
              "listen": "test",
              "script": {
                "type": "text/javascript",
                "exec": [
                  "pm.test('Status is 200', function () { pm.response.to.have.status(200); });",
                  "let json = {}; try { json = pm.response.json(); } catch (e) {}",
                  "const token = json.token || json.data?.token || json.accessToken || json.data?.accessToken || json?.payload?.token;",
                  "if (token) { pm.environment.set('token', token); pm.collectionVariables.set('token', token); }",
                  "const userId = json.user?.id || json.data?.user?.id; if (userId) pm.environment.set('userId', userId);"
                ]
              }
            }
          ],
          "request": {
            "auth": { "type": "noauth" },
            "method": "POST",
            "header": [
              { "key": "Content-Type", "value": "application/json" }
            ],
            "url": "{{base_url}}/api/auth/login",
            "body": {
              "mode": "raw",
              "raw": "{\n  \"email\": \"admin@example.com\",\n  \"password\": \"Passw0rd!\"\n}"
            }
          }
        },
        {
          "name": "Me",
          "request": {
            "method": "GET",
            "header": [ { "key": "Authorization", "value": "Bearer {{token}}" } ],
            "url": "{{base_url}}/api/auth/me"
          }
        },
        {
          "name": "Validate Token",
          "request": {
            "method": "GET",
            "header": [ { "key": "Authorization", "value": "Bearer {{token}}" } ],
            "url": "{{base_url}}/api/auth/validate"
          }
        },
        {
          "name": "Forgot Password",
          "request": {
            "auth": { "type": "noauth" },
            "method": "POST",
            "header": [ { "key": "Content-Type", "value": "application/json" } ],
            "url": "{{base_url}}/api/auth/forgot-password",
            "body": { "mode": "raw", "raw": "{\n  \"email\": \"john.doe@example.com\"\n}" }
          }
        },
        {
          "name": "Reset Password",
          "request": {
            "auth": { "type": "noauth" },
            "method": "POST",
            "header": [ { "key": "Content-Type", "value": "application/json" } ],
            "url": "{{base_url}}/api/auth/reset-password",
            "body": { "mode": "raw", "raw": "{\n  \"token\": \"{{reset_token}}\",\n  \"password\": \"NewPassw0rd!\"\n}" }
          }
        }
      ]
